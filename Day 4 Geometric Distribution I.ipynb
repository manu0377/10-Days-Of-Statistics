   "source": [
    "# Set data\n",
    "probability = list(map(int, input().split()))\n",
    "p = probability[0] / probability[1]\n",
    "q = 1 - p\n",
    "n = int(input())\n",
    "\n",
    "# Get Geometric Distribution\n",
    "result = q ** (n - 1) * p\n",
    "print(round(result, 3))"
   ]
  }
